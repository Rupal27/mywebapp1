@model IEnumerable<Shared_Library.DTO.LoginUserDTO>
@{
    ViewBag.Title = "WelcomePage";
}

<form>
    <input type="button" value="Back" onclick="history.back()">
</form>


@Html.ActionLink("Log Out", "LoginUser", "User")
<div>
    <h3> User Details </h3>
    <hr />
    <table class="table">
        <tr>

            <th>

                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>

        </tr>


        <dl class="dl-horizontal">

            @foreach (var i in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(model => @i.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(model => @i.FullName)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "EditIt", "User", new { userId = i.ID }, null)
                    </td>
                    <!--<a href="../User/Editit?UserID=@i.ID"></a>-->
                </tr>
            }


        </dl>

</div>
@if (TempData["Message"] != null)
{
    <div class="alert alert-success alert-dismissable">
        <strong>@TempData["Message"]</strong>
    </div>
}
@if (TempData["NewMessage"] != null)
{
    <div class="alert alert-success alert-dismissable">
        <strong>@TempData["NewMessage"]</strong>
    </div>
}

